{
  "name": "valorant-helper",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "explore": "source-map-explorer dist/**/*.js",
    "dev:ssr": "ng run valorant-helper:serve-ssr",
    "serve:ssr": "node dist/valorant-helper/server/main.js",
    "build:ssr": "ng build && ng run valorant-helper:server",
    "prerender": "ng run valorant-helper:prerender",
    "serve:prerender": "node dist/valorant-helper/server/main.js",
    "format": "npx prettier --write .",
    "generateRoutes": "node utils/staticRoutes.js",
    "deploy": "npm run generateRoutes && npm run prerender &&  ng deploy"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.7",
    "@angular/common": "^15.2.7",
    "@angular/compiler": "^15.2.7",
    "@angular/core": "^15.2.7",
    "@angular/fire": "^7.5.0",
    "@angular/forms": "^15.2.7",
    "@angular/platform-browser": "^15.2.7",
    "@angular/platform-browser-dynamic": "^15.2.7",
    "@angular/platform-server": "^15.2.7",
    "@angular/router": "^15.2.7",
    "@angular/service-worker": "^15.2.7",
    "@nguniversal/express-engine": "^15.2.1",
    "express": "^4.15.2",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.6",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "^15.2.6",
    "@angular/compiler-cli": "^15.2.7",
    "@nguniversal/builders": "^15.2.1",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.28.0",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "node-fetch": "^3.3.1",
    "nodemon": "^2.0.22",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-attributes": "^0.0.5",
    "pretty-quick": "^3.1.3",
    "source-map-explorer": "^2.5.2",
    "typescript": "~4.9.5"
  }
}

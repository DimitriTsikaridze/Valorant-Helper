{
  "name": "valorant-helper",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint",
    "format": "npx prettier --write .",
    "generateRoutes": "node utils/dynamicRoutes.mjs",
    "deploy": "npm run generateRoutes && npm run prerender &&  ng deploy",
    "dev:ssr": "ng run valorant-helper:serve-ssr",
    "serve:ssr": "node dist/valorant-helper/server/main.js",
    "build:ssr": "ng build && ng run valorant-helper:server",
    "prerender": "ng run valorant-helper:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.10",
    "@angular/common": "^16.2.10",
    "@angular/compiler": "^16.2.10",
    "@angular/core": "^16.2.10",
    "@angular/fire": "^7.6.1",
    "@angular/forms": "^16.2.10",
    "@angular/platform-browser": "^16.2.10",
    "@angular/platform-browser-dynamic": "^16.2.10",
    "@angular/platform-server": "^16.2.10",
    "@angular/router": "^16.2.10",
    "@angular/service-worker": "^16.2.10",
    "@nguniversal/express-engine": "^16.2.0",
    "express": "^4.15.2",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.6",
    "@angular-eslint/builder": "16.2.0",
    "@angular-eslint/eslint-plugin": "16.2.0",
    "@angular-eslint/eslint-plugin-template": "16.2.0",
    "@angular-eslint/schematics": "16.2.0",
    "@angular-eslint/template-parser": "16.2.0",
    "@angular/cli": "^16.2.6",
    "@angular/compiler-cli": "^16.2.10",
    "@nguniversal/builders": "^16.2.0",
    "@types/express": "^4.17.0",
    "@types/node": "^14.15.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.39.0",
    "node-fetch": "^3.3.1",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-attributes": "^0.0.5",
    "typescript": "~4.9.5"
  }
}